<!-- donate form start -->
    <section class="dform">
    <div class="container">
        <div class="form">
            <div class="description">
                <!-- <img class="logo"> <i class="fas fa-share-square"></i>  -->
                <h1>Share your E-Books and Material to Others With StudyLance</h1>
            

            <p class="para1"> Spread the joy and power of books by sharing </p>

             <p class="para2"> if you have any useful Engineering related eductional Contents / material with you
                kindly share with us , it will be more 
                useful to other students   
            </p>
            </div>
            
           <form action="/create-donate" method="post">
            <div class="fields">
                <div class="f1">
                    <label> Branch Name * </label>
                    <input type="text" name="branchName" required>
                    <span class="focus-input"></span>
                </div>
                <div class="f2">
                    <label> Subject Name * </label>
                    <input type="text" name="subjectName" required>
                    <span class="focus-input"></span>
                </div>
            </div>
            <div class="fields2">
                <label> File type (like Book, note, or other) </label>
                <input type="text" name="fileType">
                <span class="focus-input2"></span>
            </div>
            <div class="fields2">
                <label> File / Material URL (like GDrive, Mega or other links) * </label>
                <input type="text" name="URL" required>
                <span class="focus-input2"></span>
            </div>
            <div class="fields2">
                <label>Author /  Institute Name</label>
                <input type="text" name="author">
                <span class="focus-input2"></span>
            </div>

            <button class="btn1"> Submit </button>

        </form>  
        </div>
        <!--Image-->
        <div class="image">
            <img src="../Images/sharing2.svg" class="img" alt="sharing"/>
        </div>
    </div>
   </section> 

